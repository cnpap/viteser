{
  "name": "viteser",
  "type": "module",
  "version": "0.0.1-beta.29",
  "packageManager": "pnpm@9.1.0",
  "description": "允许直接在 vite .vue .tsx .ts 前端文件中写接口",
  "author": "杨伟杰 <sia-fl@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/sia-fl/viteser",
  "repository": {
    "type": "git",
    "url": "https://github.com/sia-fl/viteser"
  },
  "bugs": "https://github.com/sia-fl/viteser/issues",
  "keywords": [
    "use server api",
    "vite plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=v14.21.3"
  },
  "scripts": {
    "play": "pnpm -C playground run dev",
    "dev": "unbuild --stub",
    "build": "unbuild",
    "build:all": "unbuild && pnpm -C playground run build",
    "stale": "stale-del -u",
    "test": "vitest",
    "ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public --no-git-checks"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.4",
    "hono": "^4.4.7",
    "prettier": "^3.3.2",
    "tsup": "^8.0.2",
    "viteser": "^0.0.1-beta.29"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@types/node": "^20.12.7",
    "@types/signale": "^1.4.7",
    "@vercel/node": "^3.2.0",
    "@vitest/ui": "^1.5.0",
    "axios": "^1.7.2",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "http-proxy-middleware": "^3.0.0",
    "nodemon": "^3.1.0",
    "signale": "^1.4.0",
    "statl-del": "^0.0.4",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.5.0",
    "zod": "^3.23.8"
  }
}
